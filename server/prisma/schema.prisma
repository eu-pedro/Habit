// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// utiliza model porque ele suporta v√°rios bancos de dados



model Habit {
  id String @id @default(uuid())
  title String
  created_at DateTime

  @@map("habits") // nome do banco de dados
}